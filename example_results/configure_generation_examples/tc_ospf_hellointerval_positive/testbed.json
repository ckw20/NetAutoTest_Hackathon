{
  "version": {
    "description": "Version number",
    "default": "v1.0.0"
  },
  "tester": {
    "description": "Tester information",
    "product_type": {
      "description": "Renix product type: BIGTAO or DARYU",
      "default": "DARYU"
    },
    "rtsm": {
      "description": "Whether to use rtsm",
      "ip": false,
      "app": true
    },
    "force": {
      "description": "Whether to forcibly occupy the port",
      "default": true
    },
    "debug": {
      "description": "Whether to use offline mode for debugging",
      "default": false
    },
    "wait_for_status_up": {
      "description": "Whether to wait for port UP",
      "default": true
    },
    "analyzer": {
      "description": "Whether to automatically pop up the result analysis tool when executing intelligent scripts",
      "default": false
    }
  },
